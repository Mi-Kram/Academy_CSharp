<Window x:Class="NP_WCF_Client_Callback.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NP_WCF_Client_Callback"
        mc:Ignorable="d"
        Title="MainWindow" Height="497" Width="800">
    <Grid>
        <Button Content="Connect" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="158" Click="Button_Click"/>
        <Button Content="Set callback" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="158" Click="Button_Click_1"/>
        <Button Content="Get SessionId" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top" Width="158" Click="Button_Click_2"/>
        <TextBox Name="loginTextBox" HorizontalAlignment="Left" Height="23" Margin="10,91,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="158"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="10,119,0,0" VerticalAlignment="Top" Width="158" Click="Button_Click_3"/>
        <ListBox Name="messagesListBox" Margin="195,10,10,108"/>
        <TextBox Name="toTextBox" Margin="195,0,10,70" TextWrapping="Wrap" Text="" Height="23" VerticalAlignment="Bottom"/>
        <TextBox Name="messageTextBox" Margin="195,0,10,42" TextWrapping="Wrap" Text="" Height="23" VerticalAlignment="Bottom"/>
        <Button Content="Send" Margin="195,0,0,10" Height="22" VerticalAlignment="Bottom" Click="Button_Click_4" HorizontalAlignment="Left" Width="172"/>

    </Grid>
</Window>
