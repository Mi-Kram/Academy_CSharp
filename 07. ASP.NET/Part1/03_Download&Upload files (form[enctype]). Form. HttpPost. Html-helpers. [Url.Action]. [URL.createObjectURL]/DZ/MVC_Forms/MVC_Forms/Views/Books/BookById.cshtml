@model MVC_Forms.Models.Book

@{
    ViewBag.Title = "Book";
}

<h2>@Model.Title</h2>

<table id="books" class="table table-hover">
    <tbody>
        <tr>
            <td style="width:150px;"><strong>Title:</strong></td>
            <td>@Model.Title</td>
        </tr>
        <tr>
            <td><strong>Author:</strong></td>
            <td>@Model.Author</td>
        </tr>
        <tr>
            <td><strong>Genre:</strong></td>
            <td>@Model.Genre</td>
        </tr>
        <tr>
            <td><strong>Pubdate:</strong></td>
            <td>@Model.PubDate</td>
        </tr>
        <tr>
            <td><strong>Price:</strong></td>
            <td>@Model.Price</td>
        </tr>
    </tbody>
</table>

